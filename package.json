    "typescript": "3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webtest": "^0.8.3",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "google-maps": "3.2.1",
    "ol": "^6.5.0",
    "ol-mapbox-style": "6.3.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router": "4.3.1",
